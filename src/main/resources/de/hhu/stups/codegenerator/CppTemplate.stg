    if(!machine._check_inv()) {
        invariantViolated = true;
    }

    std::unordered_set\<<machine>, <machine>::Hash, <machine>::HashEqual> states = std::unordered_set\<<machine>, <machine>::Hash, <machine>::HashEqual>();
    states.insert(machine);
    std::atomic\<int> numberStates;
    numberStates = 1;

    std::list\<<machine>\> collection = std::list\<<machine>\>();
    collection.push_back(machine);

    std::atomic\<int> transitions;
    transitions = 0;

    while(!collection.empty()) {
        // TODO: BF, DF, MIXED
        <machine> state;
        if(rand() > 0.5) {
            state = collection.front();
            collection.pop_front();
        } else {
            state = collection.back();
            collection.pop_back();
        }
        std::unordered_set\<<machine>, <machine>::Hash, <machine>::HashEqual> nextStates = generateNextStates(state, transitions);
        if(nextStates.empty()) {
            deadlockDetected = true;
        }

        for(auto nextState : nextStates) {
            if(states.find(nextState) == states.end()) {
                if(!nextState._check_inv()) {
                    invariantViolated = true;
                }
                states.insert(nextState);
                collection.push_back(nextState);
                numberStates += 1;
                if(numberStates.load(std::memory_order_relaxed) % 10000 == 0) {
                    cout \<\< "VISITED STATES: " \<\< numberStates.load(std::memory_order_relaxed) \<\< "\n";
                    cout \<\< "EVALUATED TRANSITIONS: " \<\< transitions.load(std::memory_order_relaxed) \<\< "\n";
                    cout \<\< "-------------------" \<\< "\n";
                }
            }
        }
    }
    printResult(numberStates.load(std::memory_order_relaxed), transitions.load(std::memory_order_relaxed), deadlockDetected, invariantViolated.load(std::memory_order_relaxed));
    return 0;
}

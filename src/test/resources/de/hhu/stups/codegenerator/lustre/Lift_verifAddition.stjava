public static void main(String[] args) {
    Lift_verif lift = new Lift_verif();
    lift.Lift_verif_clock_step(new BBoolean(true), new BBoolean(false));
    System.out.println(lift.getFloor());
    lift.Lift_verif_clock_step(new BBoolean(false), new BBoolean(true));
    System.out.println(lift.getFloor());
    for(int i = 0; i < 100; i++) {
        lift.Lift_verif_clock_step(new BBoolean(true), new BBoolean(false));
    }
    System.out.println(lift.getFloor());
    lift.Lift_verif_clock_step(new BBoolean(true), new BBoolean(false));
    System.out.println(lift.getFloor());
    for(int i = 0; i < 100; i++) {
        lift.Lift_verif_clock_step(new BBoolean(false), new BBoolean(true));
    }
    System.out.println(lift.getFloor());
    lift.Lift_verif_clock_step(new BBoolean(false), new BBoolean(true));
    System.out.println(lift.getFloor());
}